{
    "name": "vue-audiomotion-analyzer",
    "version": "0.6.3",
    "description": "Audiomotion Analyzer plugin for Vue3",
    "author": {
        "name": "Wouter Vernaillen",
        "email": "wouter@vernaillen.com",
        "url": "https://github.com/vernaillen"
    },
    "homepage": "https://github.com/vernaillen/vue-audiomotion-analyzer",
    "repository": {
        "type": "git",
        "url": "https://github.com/vernaillen/vue-audiomotion-analyzer.git"
    },
    "bugs": {
        "url": "https://github.com/vernaillen/vue-audiomotion-analyzer/issues"
    },
    "type": "commonjs",
    "main": "index.js",
    "unpkg": "dist/vue-audiomotion-analyzer.global.js",
    "jsdelivr": "dist/vue-audiomotion-analyzer.global.js",
    "module": "dist/vue-audiomotion-analyzer.mjs",
    "types": "dist/vue-audiomotion-analyzer.d.ts",
    "exports": {
        ".": {
            "types": "./dist/vue-audiomotion-analyzer.d.ts",
            "node": {
                "import": {
                    "production": "./dist/vue-audiomotion-analyzer.prod.cjs",
                    "development": "./dist/vue-audiomotion-analyzer.node.mjs",
                    "default": "./dist/vue-audiomotion-analyzer.node.mjs"
                },
                "require": {
                    "production": "./dist/vue-audiomotion-analyzer.prod.cjs",
                    "development": "./dist/vue-audiomotion-analyzer.cjs",
                    "default": "./index.js"
                }
            },
            "import": "./dist/vue-audiomotion-analyzer.mjs",
            "require": "./index.js"
        },
        "./dist/*": "./dist/*",
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "pnpm run build:clean && pnpm run build:compile && pnpm run build:bundle && pnpm run build:dts",
        "build:clean": "rimraf dist && rimraf dist-transpiled",
        "build:dts": "api-extractor run --local --verbose",
        "build:compile": "vue-tsc --noEmit -p .",
        "build:bundle": "rollup -c --environment BUILD:production",
        "lint": "eslint src --ext .ts,vue",
        "format": "pnpm run prettier:write",
        "prettier:write": "prettier --write \"./src\"",
        "prettier:check": "prettier --check \"./src\"",
        "typecheck": "vue-tsc --noEmit -p tsconfig.json --composite false",
        "release": "semantic-release",
        "test": "jest",
        "test:watch": "jest --watchAll",
        "all": "pnpm run format && pnpm run lint && pnpm run build && pnpm run build:demo && serve -s ./demo/dist",
        "dev": "pnpm run format && pnpm run lint && pnpm run build && pnpm run dev --prefix demo",
        "publish": "npm publish"
    },
    "files": [
        "index.js",
        "dist/*.{js,cjs,mjs}",
        "dist/vue-audiomotion-analyzer.d.ts",
        "README.md"
    ],
    "keywords": [
        "vue",
        "composition-api",
        "audiomotion",
        "audiomotion-analyzer"
    ],
    "license": "MIT",
    "dependencies": {
        "audiomotion-analyzer": "^3.6.0",
        "vue": "^3.2.39"
    },
    "devDependencies": {
        "@microsoft/api-extractor": "^7.31.1",
        "@rollup/plugin-commonjs": "^22.0.2",
        "@rollup/plugin-node-resolve": "^14.1.0",
        "@rollup/plugin-replace": "^4.0.0",
        "@rushstack/eslint-patch": "^1.2.0",
        "@semantic-release/changelog": "^6.0.1",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^8.0.6",
        "@semantic-release/npm": "^9.0.1",
        "@types/jest": "^29.0.3",
        "@types/node": "^18.7.18",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.1",
        "eslint": "^8.23.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.4.0",
        "jest": "^29.0.3",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.79.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.34.0",
        "rollup-plugin-vue": "^6.0.0",
        "semantic-release": "^19.0.5",
        "ts-jest": "^29.0.1",
        "tslib": "^2.4.0",
        "typescript": "^4.8.3",
        "vue-eslint-parser": "^9.1.0",
        "vue-ts": "^1.0.11",
        "vue-tsc": "^0.40.13"
    }
}
