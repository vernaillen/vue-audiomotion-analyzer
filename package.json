{
  "name": "vama",
  "version": "0.6.2",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "Wouter Vernaillen",
  "scripts": {
    "build": "pnpm recursive run build",
    "dev": "pnpm recursive run dev",
    "lint": "pnpm recursive run lint",
    "test": "pnpm recursive run test",
    "clean": "pnpm recursive exec -- rm -rf node_modules; rm -rf node_modules",
    "plugin:build": "pnpm --filter ./plugin/ run build",
    "plugin:publish": "pnpm --filter ./plugin/ run publish",
    "docs:build": "pnpm --filter ./docs/ run build",
    "docs:dev": "pnpm --filter ./docs/ run dev",
    "demo:build": "pnpm --filter ./examples/demo/ run build",
    "demo:dev": "pnpm --filter ./examples/demo/ run dev",
    "demo-ssg:build": "pnpm --filter ./examples/demo-ssg/ run build",
    "demo-ssg:dev": "pnpm --filter ./examples/demo-ssg/ run dev",
    "demo-ssg:serve": "pnpm --filter ./examples/demo-ssg/ run serve",
    "full-test-build": "pnpm run clean && pnpm config set auto-install-peers true && pnpm install --shamefully-hoist=true && pnpm run lint && pnpm run test && pnpm run build"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=6"
  }
}