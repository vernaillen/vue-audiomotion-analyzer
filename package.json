{
    "name": "vue-audiomotion-analyzer",
    "version": "0.2.6",
    "description": "Audiomotion Analyzer plugin for Vue3",
    "author": {
        "name": "Wouter Vernaillen",
        "email": "wouter@vernaillen.com",
        "url": "https://github.com/vernaillen"
    },
    "homepage": "https://github.com/vernaillen/vue-audiomotion-analyzer",
    "repository": {
        "type": "git",
        "url": "https://github.com/vernaillen/vue-audiomotion-analyzer.git"
    },
    "bugs": {
        "url": "https://github.com/vernaillen/vue-audiomotion-analyzer/issues"
    },
    "scripts": {
        "build": "npm run build:clean && npm run build:compile && npm run build:bundle",
        "build:clean": "rimraf dist && rimraf dist-transpiled",
        "build:compile": "vue-tsc --noEmit -p .",
        "build:bundle": "rollup --config rollup.config.js",
        "lint": "eslint src --ext .ts,vue",
        "format": "npm run prettier:write",
        "prettier:write": "prettier --write \"./src\"",
        "prettier:check": "prettier --check \"./src\"",
        "release": "semantic-release",
        "test": "jest",
        "test:watch": "jest --watchAll",
        "all": "npm run format && npm run lint && npm run build && npm run build:demo && serve -s ./demo/dist",
        "dev": "npm run format && npm run lint && npm run build && npm run dev --prefix demo",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:serve": "vitepress serve docs",
        "demo:build": "cd demo && npm install && npm run build"
    },
    "main": "./dist/index.esm.js",
    "module": "./dist/index.esm.js",
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist/"
    ],
    "keywords": [
        "vue",
        "composition-api",
        "audiomotion",
        "audiomotion-analyzer"
    ],
    "license": "Apache-2.0",
    "dependencies": {
        "@rushstack/eslint-patch": "^1.1.3",
        "audiomotion-analyzer": "^3.6.0",
        "vue": "^3.2.33",
        "vue-tsc": "^0.34.15"
    },
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.1",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^8.0.4",
        "@semantic-release/npm": "^9.0.1",
        "@types/jest": "^27.4.1",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^10.0.0",
        "eslint": "^8.14.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^8.7.1",
        "jest": "^28.0.3",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.43.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-typescript2": "^0.31.2",
        "rollup-plugin-vue": "^6.0.0",
        "semantic-release": "^19.0.2",
        "ts-jest": "^28.0.0-next.3",
        "tslib": "^2.4.0",
        "typescript": "^4.6.4",
        "vitepress": "^0.22.4",
        "vue": "^3.2.33",
        "vue-eslint-parser": "^9.0.1",
        "vue-ts": "^1.0.11"
    }
}
