  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.7.18",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.1",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/head": "^0.7.11",
    "eslint": "^8.23.1",
    "eslint-plugin-vue": "^9.4.0",
    "prettier": "^2.7.1",
    "serve": "^14.0.1",
    "typescript": "~4.8.3",
    "vite": "^3.1.1",
