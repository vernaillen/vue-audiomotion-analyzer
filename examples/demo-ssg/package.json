{
  "name": "@vama/demo-ssg",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite-ssg build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "serve": "pnpm run build && serve -s dist"
  },
  "dependencies": {
    "vite-plugin-vue-audiomotion": "workspace:1.0.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.0",
    "@types/node": "^20.4.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/head": "^1.1.26",
    "prettier": "^3.0.1",
    "serve": "^14.2.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.8",
    "vite-ssg": "^0.23.1",
    "vue-tsc": "^1.8.8"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "packageManager": "pnpm@8.6.10"
}
