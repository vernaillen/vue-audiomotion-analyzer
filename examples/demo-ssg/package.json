{
  "name": "@vama/demo-ssg",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite-ssg build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "serve": "pnpm run build && serve -s dist"
  },
  "dependencies": {
    "vite-plugin-vue-audiomotion": "workspace:1.0.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.0",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/head": "^1.1.23",
    "prettier": "^2.8.7",
    "serve": "^14.2.0",
    "typescript": "~5.0.2",
    "vite": "^4.2.1",
    "vite-ssg": "^0.22.1",
    "vue-tsc": "^1.2.0"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
