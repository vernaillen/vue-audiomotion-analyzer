    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/head": "^1.0.13",
    "prettier": "^2.7.1",
    "serve": "^14.1.1",
    "typescript": "~4.8.4",
    "vite": "^3.2.3",
    "vite-ssg": "^0.21.2",
    "vue-tsc": "^1.0.9"
  },
  "eslintConfig": {
